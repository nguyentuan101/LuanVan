        <Ontology-BenhTom:hasTrieuChung rdf:resource="http://www.semanticweb.org/tuan/ontologies/2020/10/Ontology-BenhTom#Gan_tụy_trắng_đục_hoặc_hơi_vàng"/>
        <Ontology-BenhTom:hasTrieuChung rdf:resource="http://www.semanticweb.org/tuan/ontologies/2020/10/Ontology-BenhTom#Hoạt_động_yếu"/>

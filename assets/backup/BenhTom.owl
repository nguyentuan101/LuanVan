    <!-- http://www.semanticweb.org/tuan/ontologies/2020/10/Ontology-BenhTom#Bệnh_đốm_trắng -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tuan/ontologies/2020/10/Ontology-BenhTom#Bệnh_đốm_trắng">
